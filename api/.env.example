# Copy this file to .env to override defaults locally. No secrets here.
APP_ENV=development
PORT=8080
LOG_LEVEL=info
CORS_ALLOWED_ORIGINS=http://localhost:3000
SHUTDOWN_TIMEOUT=10s
SEED_ON_START=true
# DATABASE_URL format example (uncomment to use Postgres):
# DATABASE_URL=postgres://postgres:root@localhost:5432/gsdta?sslmode=disable
MIGRATE_ON_START=false
