{
  "name": "api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 8080",
    "build": "next build",
    "start": "next start -p 8080",
    "serve": "npm run build && npm run start",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test:cucumber": "cross-env TS_NODE_PROJECT=tsconfig.cucumber.json cucumber-js",
    "test:e2e": "start-server-and-test \"npm run serve\" http://localhost:8080/api/v1/health \"npm run test:cucumber\""
  },
  "dependencies": {
    "firebase-admin": "^13.5.0",
    "next": "15.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "swagger-jsdoc": "^6.2.8"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^11",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "start-server-and-test": "^2.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
