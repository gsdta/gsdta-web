{
  "name": "gsdta-web",
  "version": "1.0.0",
  "description": "GSDTA Web Application",
  "private": true,
  "packageManager": "pnpm@10.27.0",
  "scripts": {
    "seed": "pnpm --filter scripts run seed",
    "seed:clear": "pnpm --filter scripts run seed:clear",
    "emulators": "firebase emulators:start --project demo-gsdta --import=./firebase-data --export-on-exit",
    "emulators:reset": "rm -rf firebase-data && pnpm emulators",
    "dev:ui": "pnpm --filter ui run dev",
    "dev:api": "pnpm --filter api run dev",
    "dev:mobile": "./start-mobile-local.sh",
    "build:ui": "pnpm --filter ui run build",
    "build:api": "pnpm --filter api run build",
    "lint:ui": "pnpm --filter ui run lint",
    "lint:api": "pnpm --filter api run lint",
    "typecheck:ui": "pnpm --filter ui run typecheck",
    "typecheck:api": "pnpm --filter api run typecheck",
    "test:ui": "pnpm --filter ui run test",
    "test:api": "pnpm --filter api run test",
    "uat": "pnpm --filter uat run test",
    "uat:smoke": "pnpm --filter uat run test:smoke",
    "uat:ci": "pnpm --filter uat run test:ci",
    "uat:shakeout": "pnpm --filter uat run test:shakeout"
  }
}
