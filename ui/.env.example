# Public runtime config (client-side)
NEXT_PUBLIC_API_BASE_URL=https://api.dev.gsdta.org
NEXT_PUBLIC_OIDC_ISSUER=https://example-issuer
NEXT_PUBLIC_OIDC_CLIENT_ID=your-client-id
NEXT_PUBLIC_USE_MSW=true
# Optional preselected debug principal for header-based auth (id|roles|email|name)
# Example: u-admin|admin|admin@example.com|Admin User
NEXT_PUBLIC_DEBUG_USER=

# REQUIRED for production: Site URL for generating absolute URLs
# Used in metadata, structured data (JSON-LD), and asset references
# Defaults to http://localhost:3000 if not set - MUST be set in production to avoid CORS errors
NEXT_PUBLIC_SITE_URL=https://dev.gsdta.org
NEXT_PUBLIC_ANALYTICS_ID=
